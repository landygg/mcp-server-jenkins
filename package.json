{
  "name": "mcp-server-jenkins",
  "version": "0.2.1",
  "description": "Model Context Protocol server for Jenkins CI/CD integration",
  "author": "landygg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/landygg/mcp-server-jenkins.git"
  },
  "homepage": "https://github.com/landygg/mcp-server-jenkins#readme",
  "bugs": {
    "url": "https://github.com/landygg/mcp-server-jenkins/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
